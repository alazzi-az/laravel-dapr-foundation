{
    "name": "alazziaz/laravel-dapr-foundation",
    "description": "Foundation package for Laravel integration with Dapr Pub/Sub events.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "dapr/php-sdk": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.0|^8.0",
        "phpstan/phpstan": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "AlazziAz\\DaprEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlazziAz\\DaprEvents\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlazziAz\\DaprEvents\\ServiceProvider"
            ]
        }
    },
    "repositories": {
        "dapr-php-sdk": {
            "type": "vcs",
            "url": "https://github.com/dapr/php-sdk"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "pest",
        "test:coverage": "pest --coverage",
        "analyse": "phpstan analyse"
    }
}
